<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="270.0" prefWidth="930.0" style="-fx-background-radius: 30; -fx-background-color: #EAF4FE;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="HealthSchedule.controller.Routine_lowerbodyController">
   <children>
      <HBox prefHeight="270.0" prefWidth="930.0">
         <children>
            <ImageView fx:id="lowerImage" fitHeight="170.0" fitWidth="223.0" onMouseClicked="#movePage" pickOnBounds="true" preserveRatio="true">
               <HBox.margin>
                  <Insets left="20.0" top="50.0" />
               </HBox.margin>
               <image>
                  <Image url="@images/fullbodyimage.jpg" />
               </image>
            </ImageView>
            <Pane prefHeight="200.0" prefWidth="83.0" />
            <VBox prefHeight="239.0" prefWidth="431.0">
               <children>
                  <Label alignment="CENTER" prefHeight="69.0" prefWidth="374.0" styleClass="font" text="하체 운동">
                     <font>
                        <Font size="19.0" />
                     </font>
                     <VBox.margin>
                        <Insets left="20.0" />
                     </VBox.margin>
                  </Label>
                  <HBox prefHeight="42.0" prefWidth="431.0">
                     <children>
                        <JFXTextField fx:id="writeTextField" labelFloat="true" prefHeight="28.0" prefWidth="277.0" promptText="운동" />
                        <JFXButton fx:id="saveBtn" buttonType="RAISED" onAction="#saveAction" prefHeight="28.0" prefWidth="83.0" ripplerFill="#e81919" style="-fx-background-color: skyblue;" text="save" />
                        <JFXButton fx:id="timesave" buttonType="RAISED" onAction="#timesaveAction" prefHeight="28.0" prefWidth="86.0" ripplerFill="#e81919" style="-fx-background-color: skyblue;" text="time" />
                     </children>
                  </HBox>
                  <Separator prefHeight="3.0" prefWidth="359.0">
                     <VBox.margin>
                        <Insets left="10.0" right="10.0" top="10.0" />
                     </VBox.margin>
                  </Separator>
                  <Label fx:id="alarmText" alignment="CENTER" prefHeight="26.0" prefWidth="420.0" text=" ">
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                  </Label>
                  <ScrollPane fx:id="scroll" prefHeight="104.0" prefWidth="420.0">
                     <VBox.margin>
                        <Insets bottom="20.0" top="20.0" />
                     </VBox.margin>
                     <content>
                        <VBox fx:id="content" prefHeight="98.0" prefWidth="398.0" />
                     </content>
                  </ScrollPane>
               </children>
            </VBox>
            <Label fx:id="timelabel" alignment="CENTER" prefHeight="277.0" prefWidth="207.0" text="00:00:00">
               <font>
                  <Font size="25.0" />
               </font>
            </Label>
         </children>
      </HBox>
   </children>
</AnchorPane>
